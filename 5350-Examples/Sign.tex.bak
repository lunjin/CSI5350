\begin{eqnarray*} 
\alpha_{sign}(X) &=& \\
\gamma_{sign}(s)  &=& \left\{\begin{array}{lr} \{ n | n > 0\} & \mbox{if $s=+$} \\ 
                                \{0\} & \mbox{if $s=0$} \\
                                \{ n | n < 0\} & \mbox{if $s=-$} \\
                                N & \mbox{if $s=\top$} \\
                                \{\} & \mbox{if $s=\bot$} \\
                            \end{array}.
\end{eqnarray*}
